#include "GPSHandler.hpp"
#include "SatNav.hpp"
#include "LinAlg.hpp"

int main() {

    GPSHandler handler = GPSHandler("brdc0820.23n");
    SatNav problem = SatNav("GNV1B_2023-03-23_C_04.txt", "GNV" "GPS1B_2023-03-23_C_04.txt", handler);

    problem.solve();
    problem.out_errors_norm();

    return 0;
}